{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "vayu-blocks/slide-item",
  "version": "0.2.0",
  "title": "Slide Item",
  "category": "vayu-blocks",
  "description": "Create Slide template wrap",
  "parent": [
    "vayu-blocks/swipe-slider"
  ],
  "supports": {
    "reusable": false,
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "layout": false,
    "lock": true
  },
  "textdomain": "vayu-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "id": {
      "type": "string"
    },
    "uniqueId": {
      "type": "string"
    }
  },
  "usesContext": [
    "swipeslideWidth",
    "swipeslideWidthTablet",
    "swipeslideWidthMobile",
    "swipeslideWidthUnit",
    "swipeslideGap",
    "swipeslideGapTablet",
    "swipeslideGapMobile",
    "swipeslideGapUnit"
  ]
}